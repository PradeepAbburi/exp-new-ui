# Database Configuration
# You can use Neon, Supabase, or any PostgreSQL provider.
DATABASE_URL="postgres://user:password@host:port/dbname?sslmode=require"

# Session Security
# Generate a random string for this.
SESSION_SECRET="your_super_secret_session_key_here"

# Email Handling (Optional but recommended for notifications)
EMAIL_USER="your-email@gmail.com"
EMAIL_PASSWORD="your-app-password"

# Legacy Replit Auth (Can use a dummy value if running entirely on Vercel)
REPL_ID="dummy_id"
